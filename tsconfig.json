{
  "compilerOptions": {
    "lib": ["ES2020", "DOM"],
    "module": "Node16",
    "target": "ES2020",
    "moduleResolution": "Node16",
    "strict": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowImportingTsExtensions": true,
    "forceConsistentCasingInFileNames": true
  }
}
